import cdstoolbox as ct

@ct.application(title='Download data')
@ct.output.download()
def download_application():
    data = ct.catalogue.retrieve(
        'reanalysis-era5-land',
        {
            'variable': '2m_dewpoint_temperature',
            'year': '2021',
            'month': '01',
            'day': '01',
            'time': '00:00',
        }
    )
    return data
